<Window x:Class="WpfAppPresentationLayer.ToonLaatsteWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfAppPresentationLayer"
        mc:Ignorable="d"
        Title="ToonLaatsteWindow" Height="450" Width="800">
    <Grid>
        <StackPanel Orientation="Horizontal" Grid.Row="2" Grid.Column="1">
            <TextBlock x:Name="tblToonLaatste" Text="Toon laatste"></TextBlock>
            <TextBox x:Name="tbAantalTrainingen" Width="40"></TextBox>
            <TextBlock x:Name="tblTrainingen" Text="trainingen"></TextBlock>
        </StackPanel>
    </Grid>
</Window>
